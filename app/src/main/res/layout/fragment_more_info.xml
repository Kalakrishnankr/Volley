<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/llMoreInfoDetails"
        android:layout_width="match_parent"
        android:layout_height="700dp"
        android:visibility="gone">

            <RelativeLayout
                android:id="@+id/scroll_more"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:background="#efeff1"
                android:orientation="vertical"
                android:padding="5dp">

                <!-- Experience-->

                <LinearLayout
                    android:id="@+id/experience_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_experience"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/spinner_exp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp"
                        android:background="@null"/>


                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/experience_lt"
                    android:background="#A9A9A9" />

                <!-- Court Side Preference-->

                <LinearLayout
                    android:id="@+id/court_preference_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/experience_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_pref"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/spinner_pref"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp"
                        android:background="@null"/>


                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/court_preference_lt"
                    android:background="#A9A9A9" />
                <!--Position-->

                <LinearLayout
                    android:id="@+id/position_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/court_preference_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_position"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/spinner_positon"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp"
                        android:background="@null"/>


                </LinearLayout>

                <!--Height-->
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/position_lt"
                    android:background="#A9A9A9" />

                <LinearLayout
                    android:id="@+id/height_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/position_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_height"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/txtvHeight"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp"
                        android:background="@null"/>


                </LinearLayout>

                <!--Tournament Level Interest-->
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/height_lt"
                    android:background="#A9A9A9"
                    android:visibility="gone"/>

                <LinearLayout
                    android:id="@+id/interest_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/height_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:visibility="gone">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_intrest"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/spinner_tl_interest"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp"
                        android:background="@null"/>


                </LinearLayout>

                <!--Tours Played-->
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/interest_lt"
                    android:background="#A9A9A9"
                    android:visibility="gone"/>

                <LinearLayout
                    android:id="@+id/tours_played_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/interest_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:visibility="gone">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_tourplayed"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvPlayed"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:focusable="true"
                        android:maxLines="1"
                        android:inputType="textCapWords"
                        android:ellipsize="end"
                        android:nextFocusDown="@+id/spinner_tour_rating"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp"
                        android:singleLine="true"
                        android:textSize="@dimen/text_size"
                        />


                </LinearLayout>

                <!--Highest Tour Rating Earned-->


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/tours_played_lt"
                    android:background="#A9A9A9"
                    android:visibility="gone"/>

                <LinearLayout
                    android:id="@+id/tour_rating_highest_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/tours_played_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:visibility="gone">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_highest"
                        android:textStyle="bold" />

                    <Spinner
                        android:id="@+id/spinner_tour_rating"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp"
                        android:background="@null"/>


                </LinearLayout>


                <!--CBVA Player Number-->
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/tour_rating_highest_lt"
                    android:background="#A9A9A9"
                    android:visibility="gone"/>

                <LinearLayout
                    android:id="@+id/cbva_player_no_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/tour_rating_highest_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:visibility="gone">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_CBVANo"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvCBVANo"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="number"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/txtvCBVAFName"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>

                <!--CBVA First Name-->

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/cbva_player_no_lt"
                    android:background="#A9A9A9"
                    android:visibility="gone"/>

                <LinearLayout
                    android:id="@+id/cbva_first_name_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/cbva_player_no_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:visibility="gone">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_CBVAFName"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvCBVAFName"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="textCapWords"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/txtvCBVALName"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>

                <!--CBVA Last Name-->

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/cbva_first_name_lt"
                    android:background="#A9A9A9"
                    android:visibility="gone"/>

                <LinearLayout
                    android:id="@+id/cbva_last_name_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/cbva_first_name_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:visibility="gone">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_CBVALName"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvCBVALName"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="textCapWords"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/spinner_Wto_travel"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>

                <!--Willingness To Travel-->
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/cbva_last_name_lt"
                    android:background="#A9A9A9" />

                <LinearLayout
                    android:id="@+id/willingness_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/cbva_last_name_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"

                        android:gravity="center_vertical"
                        android:text="@string/hint_WtoTravel"
                        android:textStyle="bold" />


                    <Spinner
                        android:id="@+id/spinner_Wto_travel"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp"
                        android:background="@null"/>


                </LinearLayout>

                <!--High school attended-->
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/willingness_lt"
                    android:background="#A9A9A9" />

                <LinearLayout
                    android:id="@+id/high_school_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/willingness_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_highschool"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvHighschool"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="textCapWords"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/txtvIndoorClub"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>


                <!--Indoor Club played at-->

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/high_school_lt"
                    android:background="#A9A9A9" />

                <LinearLayout
                    android:id="@+id/indoor_club_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/high_school_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_indoorclub"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvIndoorClub"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="textCapWords"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/txtvColgBeach"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>
                <!--next focus changed as some of the fields are not needed but those fields will be useful for future -->

                <!--College (Club)-->

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/indoor_club_lt"
                    android:background="#A9A9A9"
                    android:visibility="gone"/>

                <LinearLayout
                    android:id="@+id/college_club_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/indoor_club_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:visibility="gone">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_collgeclub"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvColgClub"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="textCapWords"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/txtvColgBeach"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>

                <!--College (Beach)-->

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/college_club_lt"
                    android:background="#A9A9A9" />

                <LinearLayout
                    android:id="@+id/college_beach_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/college_club_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_collgebeach"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvColgBeach"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="textCapWords"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/txtvColgIndoor"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>

                <!--College (indoor)-->

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/college_beach_lt"
                    android:background="#A9A9A9" />

                <LinearLayout
                    android:id="@+id/college_indoor_lt"
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_below="@+id/college_beach_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_colgindoor"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvColgIndoor"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="textCapWords"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/topfinishes_txt_1"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>

                <!--Points-->

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_below="@+id/college_indoor_lt"
                    android:background="#A9A9A9"
                    android:visibility="gone"/>

                <LinearLayout
                    android:id="@+id/points_lt"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_below="@+id/college_indoor_lt"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:visibility="gone">

                    <com.beachpartnerllc.beachpartner.CustomTextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:text="@string/hint_points"
                        android:textStyle="bold" />

                    <com.beachpartnerllc.beachpartner.CustomEditText
                        android:id="@+id/txtvPoints"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:enabled="false"
                        android:inputType="textCapWords"
                        android:textSize="@dimen/text_size"
                        android:maxLines="1"
                        android:nextFocusDown="@+id/txtvYouLinks"
                        android:paddingLeft="5dp"
                        android:paddingStart="5dp" />


                </LinearLayout>

